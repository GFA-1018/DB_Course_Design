<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Browse" table="browse" schema="dbo" catalog="douban_db">
        <id name="relationId">
            <column name="relation_id" sql-type="int"/>
        </id>
        <property name="browseId">
            <column name="browse _id" sql-type="int"/>
        </property>
        <property name="topicsId">
            <column name="topics_id" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>