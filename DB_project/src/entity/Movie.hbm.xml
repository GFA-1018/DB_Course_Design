<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Movie" table="movie" schema="dbo" catalog="douban_db">
        <id name="movieId" column="movie_id"/>
        <property name="movieName" column="movie_name"/>
        <property name="releaseDate" column="release_date"/>
        <property name="movieTime" column="movie_time"/>
        <property name="movieIntroduction" column="movie_introduction"/>
        <property name="movieImDb" column="movie_IMDb"/>
        <property name="movieOnline" column="movie_online"/>
    </class>
</hibernate-mapping>